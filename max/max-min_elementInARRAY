#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr[n] , large , small;
    
    for(int i=0; i<n; i++)
        cin>>arr[i];
    large=small=arr[0];

for(int i=1;i<n;++i)
if (arr[i]>large)
large = arr[i];

for (int i =1; i<n; ++i)
if (arr[i] < small)
small = arr[i];

cout <<  small <<endl ;

cout << large << endl;

return 0;
}
 
